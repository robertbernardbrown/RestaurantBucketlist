{
  "name": "restaurantbucketlist",
  "version": "1.0.0",
  "description": "Allows users to create a restaurant bucketlist",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "client": "cd client && npm run start",
    "start": "./node_modules/.bin/concurrently \"./node_modules/.bin/nodemon\" \"npm run client\"",
    "build": "cd client && npm run build",
    "deploy": "yarn build && git add . && git commit -m \"Building for production\" && git push heroku master",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertbernardbrown/RestaurantBucketlist.git"
  },
  "author": "Bob",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^5.0.4",
    "chai": "^4.1.2",
    "nightmare": "^3.0.1",
    "chai-http": "^4.0.0",
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "bugs": {
    "url": "https://github.com/robertbernardbrown/RestaurantBucketlist/issues"
  },
  "homepage": "https://github.com/robertbernardbrown/RestaurantBucketlist#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-mysql-session": "^1.2.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.0.3",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  }
}